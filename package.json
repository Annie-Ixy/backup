{
  "name": "resume-screening-app",
  "version": "1.0.0",
  "description": "AI-powered resume screening and ranking system for HRBP department",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "resume",
    "screening",
    "ai",
    "hr",
    "ranking"
  ],
  "author": "HRBP Department",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "form-data": "^4.0.2",
    "iconv-lite": "^0.6.3"
  }
}
